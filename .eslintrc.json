{
  "extends": ["react-app", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-restricted-globals": [
      "error",
      {
        "name": "self",
        "message": "self is allowed only in Web Workers"
      }
    ],
    "react/react-in-jsx-scope": "off"
  },
  "env": {
    "es2020": true
  }
}
